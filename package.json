{
	"name": "svelte-query-custom",
	"version": "0.0.1-beta.3.7",
	"private": false,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package -i src && publint",
		"prepublishOnly": "npm run package",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"exports": {
		".": {
			"types": "./dist/routes/index.d.ts",
			"svelte": "./dist/routes/index.js",
			"default": "./dist/routes/index.js"
		},
		"./*": {
			"svelte": "./dist/routes/*",
			"default": "./dist/routes/*"
		},
		"./src/*": {
			"svelte": "./dist/*",
			"default": "./dist/routes/*"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"src"
	],
	"peerDependencies": {
		"svelte": "^5.0.0-next.152"
	},
	"devDependencies": {
		"@alenaksu/json-viewer": "^2.0.1",
		"@playwright/test": "^1.44.1",
		"@sveltejs/adapter-auto": "^3.2.1",
		"@sveltejs/kit": "^2.5.10",
		"@sveltejs/package": "^2.3.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/eslint": "^8.56.10",
		"big-json-viewer": "^0.1.7",
		"eslint": "^9.4.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.39.0",
		"fast-json-stable-stringify": "^2.1.0",
		"json-in-details": "^0.14.0",
		"prettier": "^3.3.1",
		"prettier-plugin-svelte": "^3.2.4",
		"publint": "^0.2.8",
		"svelte": "5.0.0-next.152",
		"svelte-check": "^3.8.0",
		"svelte-json-tree": "^2.2.0",
		"svelte-tree-view": "^1.4.2",
		"tslib": "^2.6.3",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.12.0",
		"vite": "^5.2.13",
		"vitest": "^1.6.0"
	},
	"type": "module",
	"dependencies": {
		"remeda": "^2.0.5",
		"turbo-query": "^2.2.0"
	}
}
